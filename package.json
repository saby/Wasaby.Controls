   },
   "files": [
      "Controls/*"
   ],
   "scripts": {
      "start": "node app",
      "build": "grunt build",
      "rebuild": "grunt rebuild",
      "test": "node node_modules/ws-unit-testing/mocha test-isolated",
      "test-debug": "node node_modules/ws-unit-testing/mocha %NODE_DEBUG_OPTION% test-isolated",
      "coverage": "node node_modules/ws-unit-testing/cover test-isolated"
   },
   "config": {
      "ws": "sbis3-ws",
      "viewFrom": "sbis3-ws/View",
      "viewTo": "View",
      "resources": "components",
      "tests": "tests/unit",
      "test_server_port": 1025,
      "test_report": "artifacts/xunit-report.xml",
      "test_url": {
         "scheme": "http",
         "host": "localhost",
         "port": 1025,
         "path": "",
         "query": "reporter=XUnit"
      }
   },
   "devDependencies": {
      "async": "^2.6.1",
      "autoprefixer": "^8.6.2",
      "body-parser": "^1.18.3",
