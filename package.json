      "port": 1025,
      "path": "",
      "query": "reporter=XUnit"
    }
  },
  "dependencies": {
    "async": "^2.1.4",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
