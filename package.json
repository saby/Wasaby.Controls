      "minimatch": "^3.0.3",
      "postcss": "^6.0.22",
      "progress": "^2.0.0",
      "requirejs": "2.1.18",
      "serve-static": "1.11.x",
      "typescript": "2.5.3",
      "ws-unit-testing": "git+https://git.sbis.ru/ws/unit-testing.git#release-2.0"
