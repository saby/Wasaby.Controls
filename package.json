{
   "name": "wasaby-controls",
   "version": "20.3000.0",
   "repository": {
      "type": "git",
      "url": "git@git.sbis.ru:root/sbis3-controls"
   },
   "files": [
      "Data/*",
      "Controls/*",
      "Controls-default-theme/*",
      "Controls-demo/*"
   ],
   "scripts": {
      "build": "npm run builder && node build-app",
      "build:typescript": "saby-typescript --install --tsconfig=tsconfig.base.json",
      "builder": "wasaby-cli --tasks=build --only",
      "compile": "saby-typescript --compiler",
      "start": "node app --applicationRoot=application",
      "start:units": "wasaby-cli --tasks=startTest --only --server",
      "test": "wasaby-cli --tasks=startTest --only --node --report=console -c",
      "test:node": "wasaby-cli --tasks=startTest --only --node",
      "test:node-coverage": "wasaby-cli --tasks=startTest --only --node --coverage",
      "lint": "node ./node_modules/eslint/bin/eslint --color --no-eslintrc -c=.eslintrc -f=stylish .",
      "lintless": "stylelint \"Controls/**/*.less\"",
      "update-cli-store": "wasaby-cli --tasks=initStore --only",
      "prepare": "npm run update-cli-store"
   },
   "dependencies": {},
   "devDependencies": {
      "@tensor-corp/eslint-config": "^2.0.3",
      "body-parser": "^1.19.0",
      "cookie-parser": "^1.4.3",
      "eslint": "^5.7.0",
      "express": "^4.17.1",
      "wasaby-cli": "git+https://platform-git.sbis.ru/saby/wasaby-cli.git#rc-20.3000",
      "saby-typescript": "git+https://platform-git.sbis.ru/saby/TypeScript.git#rc-20.3000",
      "saby-units": "git+https://platform-git.sbis.ru/saby/Units.git#rc-20.3000",
      "serve-static": "1.11.x",
      "stylelint": "^9.7.1",
      "stylelint-config-standard": "^18.2.0",
      "tslint": "^5.18.0",
      "fs-extra": "9.0.0"
   }
}