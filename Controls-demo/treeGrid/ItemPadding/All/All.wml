<div class="controlsDemo__wrapper">


    <div class="controlsDemo__cell controlsDemo__mr2">
        <div class="controls-text-label_theme-{{_options.theme}}">Стандартные отступы</div>
        <Controls.treeGrid:View
                keyProperty="id"
                source="{{_viewSource}}"
                columns="{{_columns}}"
                parentProperty="parent"
                nodeProperty="type"/>
    </div>

    <div class="controlsDemo__cell">
        <div class="controls-text-label_theme-{{_options.theme}}">Вертикальные отступы</div>
        <div class="controlsDemo__wrapper controlsDemo__flexRow">

            <div class="controlsDemo__cell controlsDemo__mr2">
                <div class="controls-text-label_theme-{{_options.theme}}">Отступы соответствующие размеру M</div>
                <Controls.treeGrid:View
                        keyProperty="id"
                        source="{{_viewSource}}"
                        columns="{{_columns}}"
                        parentProperty="parent"
                        nodeProperty="type">
                    <ws:itemPadding top="M" bottom="M"/>
                </Controls.treeGrid:View>
            </div>

            <div class="controlsDemo__cell controlsDemo__mr2">
                <div class="controls-text-label_theme-{{_options.theme}}">Отступы соответствующие размеру L</div>
                <Controls.treeGrid:View
                        keyProperty="id"
                        source="{{_viewSource}}"
                        columns="{{_columns}}"
                        parentProperty="parent"
                        nodeProperty="type">
                    <ws:itemPadding top="L" bottom="L"/>
                </Controls.treeGrid:View>
            </div>
        </div>
    </div>

    <div class="controlsDemo__cell">
