                        keyProperty="id"
                        source="{{_viewSource}}"
                        columns="{{_columns}}"
                        parentProperty="parent"
                        nodeProperty="type">
                    <ws:itemPadding left="null" right="null"/>
                </Controls.treeGrid:View>
            </div>

            <div class="controlsDemo__cell controlsDemo__mr2">
                <div class="controls-text-label_theme-{{_options.theme}}">Отступы соответствующие размеру S</div>
