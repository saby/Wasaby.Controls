                style="secondary"
                items="{{_items1}}"
        />
    </div>
    <div class="separator">style='unaccented'</div>
    <div class="demo-TabButtons ControlsDemo-Tabs__mainSeparate">
        <Controls.tabs:Buttons
                bind:selectedKey='SelectedKey3'
