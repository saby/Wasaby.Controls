        <Controls.tabs:Buttons
                bind:selectedKey='SelectedKey1'
                style="primary"
                keyProperty="id"
                items="{{_items1}}"
        />
    </div>
    <div class="separator">style='secondary'</div>
    <div class="demo-TabButtons ControlsDemo-Tabs__mainSeparate">
        <Controls.tabs:Buttons
