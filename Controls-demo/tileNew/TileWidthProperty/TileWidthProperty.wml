<div class="controlsDemo__wrapper controlDemo_Tile_same-width">
   <Controls.tileNew:View
      tileMode="dynamic"
      displayProperty="title"
      imageProperty="image"
      tileWidthProperty="width"
      keyProperty="id"
      source="{{_viewSource}}">
      <ws:itemTemplate>
         <ws:partial
            template="Controls/tileNew:ItemTemplate"
            hasTitle="{{itemTemplate.itemData.item.isDocument}}"/>
      </ws:itemTemplate>
   </Controls.tileNew:View>
</div>
