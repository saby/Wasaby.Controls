<ws:if data="{{menuStyle === 'defaultHead'}}">
   <div class="controls-DropdownList__head">
      <ws:partial template="{{headContentTemplate || defaultContentHeadTemplate}}" icon="{{icon}}" caption="{{caption}}"/>
   </div>
</ws:if>
<ws:if data="{{menuStyle === 'duplicateHead'}}">
   <div class="controls-MenuButton-duplicate-header {{duplicateHeadClassName}}">
      <div if="{{icon}}" class="{{icon}}"></div>
   </div>
</ws:if>
<ws:if data="{{menuStyle === 'cross'}}">
   <Controls.Button.Close class="controls-DropdownList-cross" style="light"/>
</ws:if>